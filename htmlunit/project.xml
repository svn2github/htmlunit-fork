            <groupId>commons-io</groupId>
            <artifactId>commons-io</artifactId>
            <version>1.0</version>
            <type>jar</type>
            <url>http://jakarta.apache.org/commons/io/</url>
            <properties>
                <htmlunit.group>required</htmlunit.group>
                <htmlunit.description>IO utilities</htmlunit.description>
            </properties>
        </dependency>
    </dependencies>
