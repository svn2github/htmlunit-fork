<?xml version="1.0"?>
<configuration
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="JavaScriptConfiguration.xsd">

	<class name="Anchor" extends="HTMLElement">
		<javascript min-version="1.2"/>
		<property name="name" readable="true" writable="true"/>
		<property name="text" readable="true" writable="true">
			<browser name="netscape" min-version="4" less-than-version="5"/>
		</property>
	</class>


	<class name="Button" extends="Input"/>


	<class name="Checkbox" extends="Input"/>

