<?xml version="1.0"?>
<configuration
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="JavaScriptConfiguration.xsd">

	<class name="Anchor" extends="HTMLElement">
		<javascript min-version="1.2"/>
		<property name="name" readable="true" writable="true"/>
		<property name="text" readable="true" writable="true">
			<browser name="netscape" min-version="4" less-than-version="5"/>
		</property>
	</class>


	<class name="Applet">
		<javascript min-version="1.1"/>
	</class>


	<class name="Button" extends="Input"/>


	<class name="Checkbox" extends="Input"/>

	<class name="Input" extends="HTMLElement">
		<property name="checked" readable="true" writable="true"/>
		<property name="defaultChecked" readable="true" writable="true"/>
		<property name="defaultValue" readable="true" writable="true"/>
		<property name="form" readable="true" writable="true"/>
		<property name="select" readable="true" writable="true"/>
		<property name="name" readable="true" writable="true"/>
		<property name="options" readable="true" writable="true"/>
		<property name="selectedIndex" readable="true" writable="true"/>
		<property name="type" readable="true" writable="true">
			<javascript min-version="1.1"/>
		</property>
		<property name="value" readable="true" writable="true"/>
	</class>

	<class name="Document"/><!-- NOT FINISHED -->
	<class name="FileUpload"/>
	<class name="Form"/>
	<class name="Hidden"/>
	<class name="Password"/>
	<class name="Radio"/>
	<class name="Reset"/>
	<class name="Submit"/>
	<class name="Text"/>
	<class name="Textarea"/>

	<class name="Window">
		<!-- NOT FINISHED -->
		<property name="location" readable="true" writable="false">
			<browser name="netscape" min-version="3" max-version="5"/>
			<browser name="msie" min-version="3" max-version="5"/>
			<javascript min-version="1" max-version="2"/>
		</property>

		<function name="foo">
		</function>
	</class>
</configuration>
